#
# PLEASE READ THE README FILE IN /nfs BEFORE RUNNING THESE.
#
nfs01 export TCbin=$LTPROOT/testcases/network/nfs/nfs01; nfs01
nfs02 export TCbin=$LTPROOT/testcases/network/nfs/nfs02; nfs02
nfslock01 export TCbin=$LTPROOT/testcases/network/nfs/nfslock01; nfslock01

# This will run 1 thread on 20 directories with 50 files in each.
nfsstress export TCbin=$LTPROOT/testcases/network/nfs/nfsstress; nfsstress 20 50 1

nfsstat01 export TCbin=$LTPROOT/testcases/network/nfs/nfsstat01; nfsstat01
